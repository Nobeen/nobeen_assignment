<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?> 

<GridPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="400.0" prefWidth="600.0" fx:controller="Users.UsersController">
        
    <Label text="Enter Name"  GridPane.columnIndex="0" GridPane.rowIndex="0"  ></Label>
    <TextField fx:id="name" GridPane.columnIndex="1" GridPane.rowIndex="0"  />
        
    <Label text="Enter Username"  GridPane.columnIndex="0" GridPane.rowIndex="1"  ></Label>
    <TextField fx:id="username" GridPane.columnIndex="1" GridPane.rowIndex="1"  />
        
    <Label text="Enter Password"  GridPane.columnIndex="0" GridPane.rowIndex="2" ></Label>
    <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="2"  />

    <Label text="Select Role"  GridPane.columnIndex="0" GridPane.rowIndex="3" ></Label>
    <ComboBox fx:id="role" GridPane.columnIndex="1" GridPane.rowIndex="3" >
        
        <items>
                <FXCollections fx:factory="observableArrayList" >
                </FXCollections>
        </items>
        
    </ComboBox>
        
    
    <Button onAction="#handleAddUserConfirm" text="Submit" GridPane.columnIndex="0" GridPane.rowIndex="4" alignment="BOTTOM_RIGHT"  />
        
    
</GridPane >
